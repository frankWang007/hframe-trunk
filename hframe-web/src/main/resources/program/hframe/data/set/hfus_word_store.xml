<dataset>
  <module>hframe</module>
  <code>hfus_word_store</code>
  <entity>hfus_word_store</entity>
  <name>单词库</name>
  <fields>
    <field code="hfus_word_store_id" name="单词库ID" edit-type="input" is-key="true">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="chinese_chars" name="汉字名称" edit-type="input"/>
    <field code="english_name" name="英语名称" edit-type="input"/>
    <field code="english_short_name" name="英文简称" edit-type="input"/>
    <field code="op_id" name="创建人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="create_time" name="创建时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="modify_op_id" name="修改人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="modify_time" name="修改时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="del_flag" name="删除标识" edit-type="checkbox">
      <create-edit-type>hidden</create-edit-type>
      <enum-class code="DEFAULT.BOOLEAN"/>
    </field>
  </fields>
</dataset>
