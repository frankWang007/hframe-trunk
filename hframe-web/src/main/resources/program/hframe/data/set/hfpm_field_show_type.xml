<dataset>
  <module>hframe</module>
  <code>hfpm_field_show_type</code>
  <entity>hfpm_field_show_type</entity>
  <name>展示类型【默认】</name>
  <fields>
    <field code="hfpm_field_show_type_id" name="展示类型ID" edit-type="input" is-key="true">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="hfpm_field_show_type_code" name="展示类型编码" edit-type="input"/>
    <field code="hfpm_field_show_type_name" name="展示类型名称" edit-type="input"/>
    <field code="pre_str" name="前缀" edit-type="input"/>
    <field code="after_str" name="后缀" edit-type="input"/>
    <field code="col_span" name="列数" edit-type="input"/>
    <field code="row_span" name="行数" edit-type="input"/>
    <field code="width" name="宽度" edit-type="input"/>
    <field code="height" name="高度" edit-type="input"/>
    <field code="param1" name="参数1" edit-type="input"/>
    <field code="param2" name="参数2" edit-type="input"/>
    <field code="param3" name="参数3" edit-type="input"/>
    <field code="param4" name="参数4" edit-type="input"/>
    <field code="op_id" name="创建人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="create_time" name="创建时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="modify_op_id" name="修改人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="modify_time" name="修改时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="del_flag" name="删除标识" edit-type="input">
      <create-edit-type>hidden</create-edit-type>
    </field>
  </fields>
</dataset>
