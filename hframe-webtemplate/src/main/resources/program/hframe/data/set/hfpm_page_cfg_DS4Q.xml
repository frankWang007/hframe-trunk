<dataset>
  <module>hframe</module>
  <code>hfpm_page_cfg_DS4Q</code>
  <entity>hfpm_page_cfg</entity>
  <name>项目页面设置【查询】</name>
  <fields>
    <field code="hfpm_page_cfg_id" name="页面设置ID" edit-type="input" is-key="true"/>
    <field code="hfcfg_page_template_id" name="页面模板ID" edit-type="select">
      <rel entity-code="hfcfg_page_template/hfcfg_page_template_id/hfcfg_page_template_name"/>
    </field>
    <field code="hfpm_program_cfg_id" name="项目配置ID" edit-type="select">
      <rel entity-code="hfpm_program_cfg/hfpm_program_cfg_id/show_name"/>
    </field>
    <field code="op_id" name="创建人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
    </field>
    <field code="create_time_GEQ" name="创建开始时间" edit-type="datetime"/>
    <field code="create_time_LEQ" name="创建结束时间" edit-type="datetime"/>
    <field code="modify_op_id" name="修改人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
    </field>
    <field code="modify_time_GEQ" name="修改开始时间" edit-type="datetime"/>
    <field code="modify_time_LEQ" name="修改结束时间" edit-type="datetime"/>
    <field code="del_flag" name="删除标识" edit-type="input"/>
  </fields>
</dataset>
