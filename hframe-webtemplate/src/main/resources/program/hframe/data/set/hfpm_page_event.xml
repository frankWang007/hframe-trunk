<dataset>
  <module>hframe</module>
  <code>hfpm_page_event</code>
  <entity>hfpm_page_event</entity>
  <name>页面事件【默认】</name>
  <fields>
    <field code="hfpm_page_event_id" name="页面事件ID" edit-type="input" is-key="true">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="hfpm_page_id" name="页面ID" edit-type="select">
      <rel entity-code="hfpm_page/hfpm_page_id/hfpm_page_name"/>
    </field>
    <field code="hfpm_event_name" name="事件名称" edit-type="input"/>
    <field code="hfpm_event_monitor_object" name="事件监听对象" edit-type="input"/>
    <field code="hfpm_event_monitor_object_type" name="事件监听对象类型" edit-type="input"/>
    <field code="hfpm_event_type" name="事件类型" edit-type="input"/>
    <field code="hfpm_event_effect_object" name="事件作用对象" edit-type="input"/>
    <field code="op_id" name="创建人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="create_time" name="创建时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="modify_op_id" name="修改人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="modify_time" name="修改时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="del_flag" name="删除标识" edit-type="input">
      <create-edit-type>hidden</create-edit-type>
    </field>
    <field code="hfpm_page_component_id" name="页面组件ID" edit-type="select">
      <rel entity-code="hfpm_page_component/hfpm_page_component_id/hfpm_page_component_name"/>
    </field>
  </fields>
</dataset>
