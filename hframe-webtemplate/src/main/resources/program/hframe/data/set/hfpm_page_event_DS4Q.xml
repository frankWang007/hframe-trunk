    <field code="hfpm_event_name" name="事件名称" edit-type="input"/>
    <field code="hfpm_event_monitor_object" name="事件监听对象" edit-type="input"/>
    <field code="hfpm_event_monitor_object_type" name="事件监听对象类型" edit-type="input"/>
