<dataset>
  <module>hframe</module>
  <code>hfus_entity_attr</code>
  <entity>hfus_entity_attr</entity>
  <name>常用实体属性【默认】</name>
  <fields>
    <field code="hfus_entity_attr_id" name="常用实体属性ID" edit-type="input" is-key="true">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="hfus_entity_attr_name" name="实体属性名称" edit-type="input"/>
    <field code="hfus_entity_attr_code" name="实体属性编码" edit-type="input"/>
    <field code="hfus_entity_attr_desc" name="实体描述" edit-type="input"/>
    <field code="attr_type" name="属性类型" edit-type="input"/>
    <field code="size" name="大小" edit-type="input"/>
    <field code="nullable" name="是否可为空" edit-type="input"/>
    <field code="ispk" name="是否主键" edit-type="input"/>
    <field code="is_busi_attr" name="是否业务属性" edit-type="input"/>
    <field code="op_id" name="创建人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="create_time" name="创建时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="modify_op_id" name="修改人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="modify_time" name="修改时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="del_flag" name="删除标识" edit-type="input">
      <create-edit-type>hidden</create-edit-type>
    </field>
  </fields>
</dataset>
